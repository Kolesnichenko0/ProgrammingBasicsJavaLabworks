<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="part2.labwork6.second.PrimeFactorController">
   <center>
        <AnchorPane prefHeight="425.0" prefWidth="500.0" BorderPane.alignment="CENTER">
            <children>
            <ProgressBar fx:id="progressBar" prefWidth="470.0" progress="0.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="5.0" />
                <TextArea fx:id="textAreaResults" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="35.0">
               <font>
                  <Font size="15.05" />
               </font></TextArea>
            </children>
        </AnchorPane>
   </center>
   <top>
      <FlowPane prefHeight="125.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="27.0" prefWidth="460.0" text="Welcome to Prime Factor Finder!  ">
               <font>
                  <Font size="18.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
               </FlowPane.margin>
            </Label>
            <Label prefHeight="30.0" prefWidth="40.0" text="From:">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets left="20.0" right="10.0" />
               </FlowPane.margin>
            </Label>
            <TextField fx:id="textFieldFrom" prefHeight="30.0" prefWidth="170.0">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets right="15.0" />
               </FlowPane.margin>
            </TextField>
            <Label prefHeight="30.0" prefWidth="27.0" text="To:">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets left="5.0" />
               </FlowPane.margin>
            </Label>
            <TextField fx:id="textFieldTo" prefHeight="30.0" prefWidth="188.0">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets left="5.0" right="15.0" />
               </FlowPane.margin>
            </TextField>
            <Button fx:id="buttonStart" mnemonicParsing="false" onAction="#startClicked" prefHeight="30.0" prefWidth="100.0" text="Start">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="20.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="buttonSuspend" mnemonicParsing="false" onAction="#suspendClicked" prefHeight="25.0" prefWidth="100.0" text="Pause">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="20.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="buttonResume" mnemonicParsing="false" onAction="#resumeClicked" prefHeight="25.0" prefWidth="100.0" text="Resume">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="20.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="buttonStop" mnemonicParsing="false" onAction="#stopClicked" prefHeight="25.0" prefWidth="100.0" text="Stop">
               <font>
                  <Font size="14.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="20.0" top="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </top>
</BorderPane>
